<% include ../../partials/header%>


<!-- Page Header -->
<header class="masthead" style="background-image: url('https://images.unsplash.com/photo-1494281258937-45f28753affd?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1050&q=80')">
    <div class="overlay"></div>
    <div class="container">
    <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
        <div class="page-heading">
            <h1 class="intrans"><%=series[0].title%></h1>
            <span class="subheading sub_animation"><%=series[0].application%></span>

        </div>
        </div>
    </div>
    </div>
</header>

    <!-- Main Content -->
    <div class="container sub_animation">
      <div class="row">
        <%products.forEach(function(product){ %>
              <div class="col-md-3 col-4">
                <div class="card mb-4" style="height:240px;">
                  <img class="card-img-top" style="height: 180px; width: 100%; display: block;background-color: <%=product.hexa%>" >
                  <div class="card-body">
                    <h5 class="card-title text-center font-weight-normal" style="font-size:12px;text-align:center"><%=product.title%></h5>
                    
                  </div>
                </div>
              </div>
              
        <% }); %>
      </div>
    </div>

    <hr>
    
    <% include ../../partials/footer %>

 